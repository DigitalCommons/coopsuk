{
  "include": [
    "node_modules/mykomap",
    "src/**/*"
  ],
  "compilerOptions": {
    "outDir": "./build/",
    "paths": {
      "mykomap/*": ["./node_modules/mykomap/www/map-app/*","../www/map-app/*"]
    },
    "noImplicitAny": true,
    "sourceMap": true,
    "target": "es2019",
    "lib": ["DOM", "ES2021"],
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  }
}
